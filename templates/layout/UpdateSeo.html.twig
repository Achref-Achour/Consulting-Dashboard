{% extends 'layout/backLayout.html.twig' %}
{% block stylesheets %}
{{ parent() }}
   <link href={{ asset('new/plugins/bootstrap-touchspin/jquery.bootstrap-touchspin.css')}} rel="stylesheet" type="text/css" />
   <link href={{ asset('new/plugins/daterangepicker/daterangepicker.css')}} rel="stylesheet" type="text/css" />
   <link href={{ asset('new/plugins/select2/select2.min.css')}} rel="stylesheet" type="text/css" />
   <link href={{ asset('new/plugins/switchery/switchery.min.css')}} rel="stylesheet" type="text/css" />
   <link href={{ asset('new/plugins/bootstrap-colorpicker/bootstrap-colorpicker.min.css')}} rel="stylesheet" type="text/css" />
   <link href={{ asset('new/plugins/bootstrap-datepicker/bootstrap-datepicker.min.css')}} rel="stylesheet" type="text/css" />

{% endblock %}
{% block body %}
<div class="main-content">

            <div class="page-content">
                <div class="container-fluid">
                                    <div class="row">
                            <div class="col-xl-12">
                            <div class="card">
                                <div class="card-body">
                                    

                                    <form id="UpSeo" action="{{ path('UpdateSeo',{id: seo.id})}}" method="POST" enctype="multipart/form-data">
                                        <div class="form-group">
                                            <label for="simpleinput">Page</label>
                                            <input type="text" name="page" id="page" class="form-control" placeholder="Enter your text" value="{{ seo.page }}">
                                        </div>

                                        <div class="form-group">
                                            <label for="simpleinput">Title</label>
                                            <input type="text" name="title" id="title" class="form-control" placeholder="Enter your text" value="{{ seo.title }}">
                                        </div>

                                         <div class="form-group">
                                            <label for="simpleinput">keywords</label>
                                            <input type="text" name="keywords" id="keywords" class="form-control" placeholder="Enter your text" value="{{ seo.keywords }}">
                                        </div>
                                        
                                        <div class="form-group">
                                            <label for="simpleinput">Description</label>
                                            <input type="text" name="description" id="description" class="form-control" placeholder="Enter your text" value="{{ seo.description }}">
                                        </div>
                                        
                                       
                                        <button class="btn btn-primary waves-effect waves-light" type="submit">Submit Form</button>  
                                    </form>
                                </div>
                                <!-- end card-body-->
                            </div>
                            <!-- end card -->

                            
                            </div> <!-- end card-->
                                        </div> <!-- end col -->
                                    </div> <!-- end row -->
                                </div> <!-- end .w-100 -->
                            </div> <!-- end .d-flex -->
                        </div> <!-- end col-->
                    </div> <!-- end row -->
                </div>
                <!-- end container -->
            </div>
  </div>  </div>

{% endblock %}

{% block javascripts %}
{{ parent() }}
    <!-- Plugins js -->
    <script src={{ asset('new/plugins/autonumeric/autoNumeric-min.js')}}></script>
    <script src={{ asset('new/plugins/bootstrap-datepicker/bootstrap-datepicker.min.js')}}></script>
    <script src={{ asset('new/plugins/bootstrap-maxlength/bootstrap-maxlength.min.js')}}></script>
    <script src={{ asset('new/plugins/moment/moment.js')}}></script>
    <script src={{ asset('new/plugins/daterangepicker/daterangepicker.js')}}></script>
    <script src={{ asset('new/plugins/select2/select2.min.js')}}></script>

    <script src={{ asset('new/plugins/switchery/switchery.min.js')}}></script>
    <script src={{ asset('new/plugins/bootstrap-colorpicker/bootstrap-colorpicker.min.js')}}></script>
    <script src={{ asset('new/plugins/bootstrap-touchspin/jquery.bootstrap-touchspin.min.js')}}></script>


    <!-- Custom Js -->
    <script src={{ asset('new/pages/advanced-plugins-demo.js')}}></script>
 

    <!-- App js -->
    <script src={{ asset('new/js/theme.js')}}></script>

{% endblock %}
